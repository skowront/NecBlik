<UserControl x:Class="ZigBee.Views.Controls.ZigBeeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ZigBee.Views.Controls"
             xmlns:diagram="clr-namespace:DiagramDesigner;assembly=DiagramsExtension"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Path Style="{StaticResource Process}" ToolTip="Process">
        <diagram:DesignerItem.DragThumbTemplate>
            <ControlTemplate>
                <Path Style="{StaticResource Process_DragThumb}"/>
            </ControlTemplate>
        </diagram:DesignerItem.DragThumbTemplate>
        
    </Path>
</UserControl>
