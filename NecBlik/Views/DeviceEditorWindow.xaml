<Window x:Class="NecBlik.Views.DeviceEditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NecBlik.Views"
        mc:Ignorable="d"
        Title="DeviceEditorWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="Name"></TextBlock>
            <TextBox Text="{Binding ViewModel.Name}"></TextBox>
            <TextBlock Text="Version"></TextBlock>
            <TextBox Text="{Binding ViewModel.Version}"></TextBox>
        </StackPanel>
    </Grid>
</Window>
